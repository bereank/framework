    }
  ],
  "replace": {
    "leysco100/shared": "self.version",
    "leysco100/gpm": "self.version"
  },
  "require-dev": {
    "orchestra/testbench": "^7.6",
    "pestphp/pest": "^1.21",
    "phpunit/phpunit": "^9.5",
    "pestphp/pest-plugin-laravel": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Leysco100\\Shared": "src/Leysco100/Shared",
      "Leysco100\\Gpm": "src/Leysco100/Gpm"
    }
  },

  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
