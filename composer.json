{
  "name": "leyscoltd/framework",
  "description": "The Leysco Framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Berean Kibet",
      "email": "bereankibet@gmail.com"
    }
  ],
  "replace": {
    "leysco100/shared": "self.version",
    "leysco100/gpm": "self.version"
  },
  "require-dev": {
    "orchestra/testbench": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Leysco100\\Shared\\": "src/Leysco100/Shared",
      "Leysco100\\Administration\\": "src/Leysco100/Administration",
      "Leysco100\\Banking\\": "src/Leysco100/Banking",
      "Leysco100\\Gpm\\": "src/Leysco100/Gpm",
      "Leysco100\\BusinessPartner\\": "src/Leysco100/BusinessPartner",
      "Leysco100\\Inventory\\": "src/Leysco100/Inventory",
      "Leysco100\\MarketingDocuments\\": "src/Leysco100/MarketingDocuments",
      "Leysco100\\Finance\\": "src/Leysco100/Finance"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "spatie/laravel-multitenancy": "3.0.3",
    "maatwebsite/excel": "^3.1",
    "spatie/laravel-permission": "^5.10",
    "simplesoftwareio/simple-qrcode": "^4.2"
  }
}
